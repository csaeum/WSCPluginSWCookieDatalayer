  "extra": {
    "description": {
      "de-DE": "OpenSource CookieConsent, DataLayer & TagManager",
      "en-GB": "OpenSource CookieConsent, DataLayer & TagManager",
      "fr-FR": "OpenSource CookieConsent, DataLayer & TagManager"
    },
    "label": {
      "de-DE": "WSC | CookieConsent + DataLayer + TagManager",
      "en-GB": "WSC | CookieConsent + DataLayer + TagManager",
      "fr-FR": "WSC | CookieConsent + DataLayer + TagManager"
    },
    "shopware-plugin-class": "WSC\\SWCookieDataLayer\\WscSwCookieDataLayer",
    "supportLink": {
      "de-DE": "https://www.Web-SEO-Consulting.eu/support",
      "en-GB": "https://www.Web-SEO-Consulting.eu/support",
      "fr-FR": "https://www.Web-SEO-Consulting.eu/support"
    },
    "manufacturerLink": {
      "de-DE": "https://www.Web-SEO-Consulting.eu",
      "en-GB": "https://www.Web-SEO-Consulting.eu",
      "fr-FR": "https://www.Web-SEO-Consulting.eu"
    }
  },
  "require": {
    "shopware/core": "~6.5.0|~6.6.0|~6.7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "symfony/phpunit-bridge": "^6.0",
    "friendsofphp/php-cs-fixer": "^3.0",
    "phpstan/phpstan": "^1.10"
  },
  "scripts": {
    "test": "phpunit",
    "test-coverage": "phpunit --coverage-html coverage",
    "cs-check": "php-cs-fixer fix --dry-run --diff",
    "cs-fix": "php-cs-fixer fix",
    "phpstan": "phpstan analyse --memory-limit=1G",
    "ci": [
      "@cs-check",
      "@phpstan",
      "@test"
    ]
  }
}
