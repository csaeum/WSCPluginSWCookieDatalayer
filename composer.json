  ],
  "autoload": {
    "psr-4": {
      "WSC\\SWCookieDataLayer\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "WSC\\SWCookieDataLayer\\Tests\\": "tests/"
    }
